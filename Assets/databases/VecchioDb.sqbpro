<?xml version="1.0" encoding="UTF-8"?><sqlb_project><db path="VecchioDb.db" readonly="0" foreign_keys="1" case_sensitive_like="0" temp_store="0" wal_autocheckpoint="1000" synchronous="2"/><attached/><window><main_tabs open="structure browser pragmas query" current="0"/></window><tab_structure><column_width id="0" width="300"/><column_width id="1" width="0"/><column_width id="2" width="100"/><column_width id="3" width="3285"/><column_width id="4" width="0"/><expanded_item id="0" parent="1"/><expanded_item id="1" parent="1"/><expanded_item id="2" parent="1"/><expanded_item id="3" parent="1"/></tab_structure><tab_browse><table title="spartiti" custom_title="0" dock_id="5" table="4,8:mainspartiti"/><dock_state state=""/><default_encoding codec=""/><browse_table_settings><table schema="main" name="spartiti" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="58"/><column index="2" value="38"/><column index="3" value="46"/><column index="4" value="68"/><column index="5" value="50"/><column index="6" value="64"/><column index="7" value="60"/><column index="8" value="54"/><column index="9" value="56"/><column index="10" value="124"/><column index="11" value="52"/><column index="12" value="48"/><column index="13" value="57"/><column index="14" value="91"/><column index="15" value="132"/><column index="16" value="93"/><column index="17" value="117"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">CREATE TABLE spartiti (
    id_brano INTEGER PRIMARY KEY AUTOINCREMENT, -- Chiave primaria univoca per il brano all'interno di questo catalogo
    titolo TEXT NOT NULL,                     -- Titolo del brano
    autore TEXT,-- Autore/Compositore del brano
    strumento_traspositore TEXT,              -- Strumento a cui si riferisce lo spartito e sua eventuale trasposizione (es. &quot;Sax Alto Eb&quot;, &quot;Tromba Bb&quot;, &quot;Violino&quot;, &quot;Pianoforte&quot;)
    volume_pdf TEXT NOT NULL,                 -- Nome del file PDF o del volume/libro che contiene lo spartito (senza percorso base)
	PrimoLink TEXT,                        -- (Opzionale) URL a una risorsa esterna (es. YouTube, Wikipedia, sito dell'artista)
	tonalita TEXT,                            -- Tonalità originale o di esecuzione del brano (es. &quot;Cm&quot;, &quot;Gmaj&quot;, &quot;Dorico&quot;)
    genere TEXT,                              -- Genere musicale (es. &quot;Jazz&quot;, &quot;Blues&quot;, &quot;Pop&quot;, &quot;Classica Barocca&quot;)
    numero_pagina_pdf INTEGER NOT NULL,       -- Numero di pagina all'interno del `volume_pdf` dove inizia lo spartito
    numero_pagina_originale TEXT,             -- (Opzionale) Numero di pagina originale stampato sul libro/spartito, se diverso
    provenienza_dati TEXT,                    -- (Opzionale) Origine dell'inserimento (es. &quot;CSV Import&quot;, &quot;Manuale&quot;, &quot;Nome File CSV&quot;)
    difficolta TEXT,                          -- (Opzionale) Livello di difficoltà stimato (es. &quot;Facile&quot;, &quot;Medio&quot;, &quot;Difficile&quot;, &quot;1-5&quot;)
    data_aggiunta TEXT DEFAULT (STRFTIME('%Y-%m-%d %H:%M:%S', 'NOW', 'localtime')), -- Data di aggiunta del brano a questo catalogo
    data_ultima_modifica TEXT,                -- Data dell'ultima modifica dei dati di questo brano
    note_personali TEXT,                      -- (Opzionale) Campo per appunti personali dell'utente sul brano
    id_univoco_globale TEXT UNIQUE            -- (Opzionale ma Raccomandato) Un ID univoco per il brano che potrebbe essere usato per sincronizzazione o identificazione trasversale ai cataloghi, se necessario (es. UUID)
    -- Potresti aggiungere altri campi specifici che ti vengono in mente, come:
    -- bpm INTEGER,                            -- Battiti per minuto (tempo)
    -- durata_stimata TEXT,                    -- Durata stimata dell'esecuzione (es. &quot;3:45&quot;)
    -- anno_composizione INTEGER,              -- Anno di composizione
    -- preferito BOOLEAN DEFAULT 0,            -- Flag per marcare come preferito (0 = false, 1 = true)
);

</sql><current_tab id="0"/></tab_sql></sqlb_project>
